			"sources": [
				{
					"name": "SourceProcessed"
				}
			],
			"sinks": [
				{
					"name": "SinkTest"
				}
			],
			"transformations": [
				{
					"name": "FilterByParam"
				}
			],
			"scriptLines": [
				"parameters{",
